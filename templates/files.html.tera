{% extends "base" %}
{% block content %}
  <h1>Show {{ files_total }} Files</h1>
  {% if files.has_before %}
    <a href="/files?before={{files.before}}">before</a>
  {% endif %}
  {% if files.has_after %}
    <a href="/files?after={{files.after}}">next</a>
  {% endif %}
  <br />
  {% for file in files.files %}
    <a href="{{ camera_file_url(camera_id=file.camera_id, file_path=file.file_path) }}" target="_blank">
      <img width="200px" src="{{ camera_file_url(camera_id=file.camera_id, file_path=file.file_path) }}"  alt="">
    </a>
  {% endfor %}
  <pre>{{ files | json_encode(pretty=true) | safe }}</pre>
{% endblock content %}
